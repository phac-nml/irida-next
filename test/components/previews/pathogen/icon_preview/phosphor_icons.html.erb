<div class="p-6">
  <div
    class="
      grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 xl:grid-cols-6
      gap-6 items-stretch
    "
  >
    <% [
      'arrow-down',
      'arrow-left',
      'arrow-right',
      'arrow-up',
      'bank',
      'calendar-dots',
      'caret-down',
      'caret-left',
      'caret-right',
      'caret-up',
      'check',
      'check-circle',
      'clipboard-text',
      'clock',
      'dots-three-outline-vertical',
      'download-simple',
      'export',
      'eye',
      'eye-slash',
      'file',
      'file-magnifying-glass',
      'file-text',
      'files',
      'flask',
      'folder-open',
      'gear-six',
      'info',
      'list',
      'list-bullets',
      'lock-key',
      'magnifying-glass',
      'plus',
      'plus-circle',
      'question',
      'robot',
      'rocket-launch',
      'sidebar',
      'sliders-horizontal',
      'squares-four',
      'stack',
      'table',
      'terminal-window',
      'ticket',
      'translate',
      'user-circle',
      'users',
      'warning-circle',
      'x',
      'x-circle'
    ].each do |icon_name| %>
      <div class="flex flex-col items-center space-y-1 min-w-[10rem] p-2">
        <%= render Pathogen::Icon.new(icon_name) %>
        <span class="text-xs text-slate-600 font-medium"><%= icon_name %></span>

      </div>
    <% end %>
  </div>
</div>
