<div
  class="<%= class_names("namespace-entry", "has-description": @namespace.description.present?) %>"
  role="treeitem"
>
  <div class="flex items-center py-3 pr-3 namespace-entry-contents">
    <span class="inline-block">
      <span class="size-3"></span>
    </span>
    <%= render NamespaceTree::Row::RowContentsComponent.new(
      namespace: @namespace,
      path: @path,
      path_args: @path_args,
      icon_size: @icon_size,
      search_params: @search_params,
    ) %>
  </div>
</div>
