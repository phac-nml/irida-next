<%= render PaginationComponent.new(
  info: pagy_info(@pagy),
  prev_url:
    (
      if @pagy.prev
        workflow_executions_url(page: @pagy.prev)
      else
        nil
      end
    ),
  next_url:
    (
      if @pagy.next
        workflow_executions_url(page: @pagy.next)
      else
        nil
      end
    )
) %>
