<% if @page == 1 %>
  <%= turbo_stream.replace "list_selections" do %>
    <%= turbo_frame_tag "list_selections" %>
  <% end %>
<% end %>

<%= turbo_stream.append("list_selections") do %>
  <% @deletable_workflow_executions.each do |workflow_execution| %>
    <%= render "shared/workflow_executions/list_workflow_execution",
    workflow_execution: workflow_execution %>
  <% end %>
<% end %>

<%= turbo_stream.append("list_non_deletable_selections") do %>
  <% @non_deletable_workflow_executions.each do |workflow_execution| %>
    <%= render "shared/workflow_executions/list_workflow_execution",
    workflow_execution: workflow_execution %>
  <% end %>
<% end %>
