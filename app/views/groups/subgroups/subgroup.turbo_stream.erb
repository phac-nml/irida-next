<%= turbo_stream.replace dom_id(@group), method: :morph do %>
  <%= render NamespaceTree::Row::WithChildrenComponent.new(
    namespace: @group,
    children: @children,
    collapsed: @collapsed,
    path: "group_subgroups_path",
    path_args: {
      group_id: @group.full_path,
    },
    type: [Group.sti_name, Namespaces::ProjectNamespace.sti_name],
  ) %>
<% end %>
