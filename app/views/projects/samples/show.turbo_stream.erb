<%= turbo_stream.update "table-listing" do %>
  <% if @tab == 'metadata' %>
    <% render partial: "projects/samples/metadata/table",
    locals: {
      sample_metadata: @table_listing
    } %>
  <% else %>
    <% render partial: "projects/samples/attachments/table",
    locals: {
      attachments: @table_listing
    } %>
  <% end %>
<% end %>
